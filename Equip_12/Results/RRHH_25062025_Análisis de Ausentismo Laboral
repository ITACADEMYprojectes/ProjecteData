    "# Comparativa de los 3 datasets en uno solo\n",
    "merged = hours_per_month.merge(days_per_month, on='Month_absence_name')\n",
    "merged = merged.merge(count_per_month, on='Month_absence_name')\n",
    "plt.figure(figsize=(12,6))\n",
    "plt.plot(merged['Month_absence_name'], merged['Absenteeism_hours'], marker='o', label='Horas')\n",
    "plt.plot(merged['Month_absence_name'], merged['Estimated_Absence_Days'], marker='s', label='Días')\n",
    "plt.plot(merged['Month_absence_name'], merged['Absence_Records'], marker='^', label='Ausencias')\n",
    "plt.title(\"Comparativa mensual de indicadores de absentismo\")\n",
    "plt.ylabel(\"Valor\")\n",
    "plt.xticks(rotation=45)\n",
    "plt.legend()\n",
    "plt.tight_layout()\n",
