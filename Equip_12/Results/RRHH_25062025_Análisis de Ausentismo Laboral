    "ax = sns.barplot(data=seasonal_hours, x='Seasons_name', y='Absenteeism_hours')\n",
    "plt.title(\"Horas de absentismo por estación del año\")\n",
    "plt.ylabel(\"Horas\")\n",
    "plt.xlabel(\"Estación del año\")\n",
