   "cell_type": "code",
   "execution_count": 31,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 700x500 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Dias de absentismo por estaciones del año\n",
    "seasonal_days = (\n",
    "    pki.groupby('Seasons_name', observed=True)['Estimated_Absence_Days']\n",
    "    .sum()\n",
    "    .round(0)\n",
    "    .astype(int)\n",
    "    .reset_index()\n",
    ")\n",
    "plt.figure(figsize=(7,5))\n",
    "ax = sns.barplot(data=seasonal_days, x='Seasons_name', y='Estimated_Absence_Days')\n",
    "plt.title(\"Días estimados de absentismo por estación del año\")\n",
    "plt.ylabel(\"Días\")\n",
    "plt.xlabel(\"Estación del año\")\n",
    "plt.tight_layout()\n",
    "\n",
    "for p in ax.patches:\n",
    "    height = p.get_height()\n",
    "    ax.text(\n",
    "        p.get_x() + p.get_width() / 2,\n",
    "        height + 1,\n",
    "        f'{int(height)}',\n",
    "        ha='center'\n",
    "    )\n",
    "plt.show()"
