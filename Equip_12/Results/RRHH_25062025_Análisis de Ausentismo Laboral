    "# Media de dias de asuencia por tramos de edad:\n",
    "\n",
    "# Tramos de 3 años\n",
    "df_laboral['Tramo_3_años'] = pd.cut(\n",
    "    df_laboral['Service_time'],\n",
    "    bins=[0, 3, 6, 9, 12, 15, 18, 21, 24, 30],\n",
